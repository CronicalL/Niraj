public class Practice2
{
	int n;
	String s;
	public void setN(int n1)
	{
	    n=n1;
	}
	public void setS(String s1)
	{
		s=s1;
	}
	public int getN()
	{
		return n;
	}
	public String getS()
	{
		return s;
	}
	public static void main(String args[])
	{
		Practice2 gs = new Practice2();
		gs.setN(10);
		gs.setS("Smit");
		System.out.println("Entered number is:"+gs.getN());
		System.out.println("Enetered number is:"+gs.getS());
	}
}
